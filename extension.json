{
	"name": "Scratchblocks for Scratch 3.0",
	"version": "3.1-scratch3.1.0",
	"author": [
		"ErnieParke",
		"blob8108",
		"apple502j"
	],
	"url": "https://github.com/apple502j/mw-ScratchBlocks4",
	"descriptionmsg": "sb3-desc",
	"license-name": "MIT",
	"type": "parserhook",
	"AutoloadClasses": {
		"Scratchblock4Hook": "ScratchblockHooks.php"
	},
	"ExtensionFunctions": [
		"Scratchblock4Hook::sbSetup"
	],
	"ResourceModules": {
		"ext.scratchBlocks": {
			"scripts": [
				"js/scratchblocks-v4-min.js",
				"js/translations-v4-min.js",
				"run_scratchblocks.js"
			],
			"styles": "/inline.css",
			"dependencies": []
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "",
		"remoteExtPath": "ScratchBlocks4"
	},
	"Hooks": {
		"ParserFirstCallInit": [
			"Scratchblock4Hook::sbParserInit"
		],
		"ResourceLoaderGetConfigVars": [
			"Scratchblock4Hook::sbReadLS"
		]
	},
	"MessagesDirs": {
		"ScratchBlocks4": [
			"i18n"
		]
	},
	"config": {
        "ScratchBlocks4Lang": "en"
    },
	"manifest_version": 1
}
